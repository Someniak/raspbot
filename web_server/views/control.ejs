<% include partials/header.ejs %>
<body>
<div class="container">
    <h1>Control</h1>
    <a href="/overview">Back to current connections</a>
    <div class="row">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title">Information</h3>
            </div>
            <div class="panel-body">
                <span class="label label-default">IDENTIFIER:</span> <%= connection.id %><br>
                <span class="label label-default">FRIENDLY NAME:</span> <%= connection.name %><br>
                <span class="label label-default">IP-ADDRESS:</span> <%= connection.ip %><br>
                <span class="label label-default">PORT:</span> <%= connection.port %><br>

            </div>
        </div>
    </div>
</div>
</body>
